       </router-link>\n\n            <router-link to=\"/admin\">\n              <b-button size=\"sm\" class=\"my-2 mr-2 my-sm-0\">\n                <i class=\"fas fa-user\"></i> Admin\n              </b-button>\n            </router-link>\n\n            <router-link to=\"/message\">\n              <b-button size=\"sm\" class=\"my-2 mr-2 my-sm-0\">\n                <i class=\"fas fa-envelope\"></i> Message\n              </b-button>\n            </router-link>\n\n            <router-link to=\"/clients\">\n              <b-button size=\"sm\" class=\"my-2 mr-2 my-sm-0\">\n                <i class=\"fas fa-users\"></i> clients\n              </b-button>\n            </router-link>\n          </b-nav-form>\n        </b-navbar-nav>\n      </b-collapse>\n    </b-navbar>\n\n    <b-container fluid class=\"bv-example-row mt-4\">\n      <b-row>\n        <b-col cols=\"12\">\n          <b-table :fields=\"fieldsClient\" bordered striped hover :items=\"clients\">\n            <template v-slot:cell(delete)=\"row\">\n              <b-button\n                size=\"md\"\n                variant=\"danger\"\n                block\n                @click=\"deletekey('clients',row.item.id)\"\n              >Delete Client</b-button>\n            </template>\n            <template v-slot:cell(edit)=\"row\">\n              <b-button\n                size=\"md\"\n                variant=\"info\"\n                block\n                @click=\" modaldata('Edit',row.item.id)\"\n              >Edit Reservation</b-button>\n            </template>\n          </b-table>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n\n<script>\nimport { db } from \"@/firebase\";\n//import Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      clients: [],\n      fieldsClient: [\n        //  { key: \"id\", label: \"Client id\", sortable: true },\n        { key: \"lastname\", label: \"lastname\", sortable: true },\n        { key: \"firstname\", label: \"firstname\", sortable: true },\n        { key: \"nationality\", label: \"nationality\", sortable: true },\n        { key: \"phone\", label: \"phone\", sortable: true },\n        { key: \"email\", label: \"E-mail\", sortable: true },\n        { key: \"room\", label: \"Room\", sortable: true }\n      ]\n    };\n  },\n  created() {\n    db.collection(\"clients\").onSnapshot(querySnapshot => {\n      this.clients = [];\n      querySnapshot.forEach(doc => {\n        const x = doc.data();\n        x.id = doc.id;\n        this.clients.push(x);\n      });\n    });\n  },\n\n  methods: {}\n};\n</script>\n\n<style scoped>\ni {\n  color: #48a9a6;\n}\nimg {\n  height: 20xvh !important;\n  width: 100%;\n}\n</style>\n"]}]}