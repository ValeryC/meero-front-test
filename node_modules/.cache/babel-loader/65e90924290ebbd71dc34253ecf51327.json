C,gBAAA,QAAQ,EAAR,EAAiC;AAAxB,gBAAA,QAAQ,EAAR;AAAwB,eAAnF,E,IAAA,E;;AAAA,gB,cAAA,CAAmF,CAAnF;AAAA,a;;WADF,CAxDe,EA2Df,YAAA,CAAiF,mBAAjF,EAAiF;AAAvE,YAAA,KAAK,EAAC,YAAiE;AAApD,YAAA,KAAK,EAAL,EAAoD;AAA9C,YAAA,IAAI,EAAC,QAAyC;AAA/B,YAAA,OAAO,EAAE,KAAA,CAAA;AAAsB,WAAjF,E;6BAAkE;AAAA,qBAAI,C,WAAA,CAAJ;AAAA,a;;WAAlE,E;;AAAA,Y,WAAA,CA3De,CARf;AAAA,S,CADuB;;AAAA,OAAzB,E;;AAAA,Q,YAAA,CAqES,CArET;AAAA,K;;GADF,E;;AAAA,I,SAAA,CAwEI,CAvLN,C","sourcesContent":["<template>\n  <div>\n    <b-container fluid class=\"bv-example-row mt-4\">\n      <b-row>\n        <b-col>\n          <b-card no-body class=\"overflow-hidden\">\n            <b-row no-gutters>\n              <b-col md=\"6\">\n                <b-card-img :src=\"require('../assets/img1.jpg')\" class=\"rounded-0\"></b-card-img>\n              </b-col>\n              <b-col md=\"6\">\n                <b-card-body class=\"text-primary\" title=\"Single\">\n                  <b-card-text class=\"text-dark\">\n                    <h2 class=\"mt-4\">Room with wifi</h2>\n                    <h6 class=\"mt-4\">1 Person</h6>\n                    <p class=\"mt-4\">Individual space to rest.</p>\n                  </b-card-text>\n                  <b-button\n                    @click=\"modaldata('Single',500,'primary')\"\n                    size=\"lg\"\n                    class=\"mt-4\"\n                    block\n                    variant=\"primary\"\n                  >Book</b-button>\n                </b-card-body>\n              </b-col>\n            </b-row>\n          </b-card>\n        </b-col>\n\n        <b-col>\n          <b-card no-body class=\"overflow-hidden\">\n            <b-row no-gutters>\n              <b-col md=\"6\">\n                <b-card-img :src=\"require('../assets/img2.jpg')\" class=\"rounded-0\"></b-card-img>\n              </b-col>\n              <b-col md=\"6\">\n                <b-card-body class=\"text-success\" title=\"Double\">\n                  <b-card-text class=\"text-dark\">\n                    <h2 class=\"mt-4\">Double Room</h2>\n                    <h6 class=\"mt-4\">2 Persons</h6>\n                    <p class=\"mt-4\">Popular Room.</p>\n                  </b-card-text>\n                  <b-button\n                    @click=\"modaldata('Double',700,'success')\"\n                    size=\"lg\"\n                    class=\"mt-4\"\n                    block\n                    variant=\"success\"\n                  >Book</b-button>\n                </b-card-body>\n              </b-col>\n            </b-row>\n          </b-card>\n        </b-col>\n      </b-row>\n\n      <b-row class=\"mt-4 mb-4\">\n        <b-col>\n          <b-card no-body class=\"overflow-hidden\">\n            <b-row no-gutters>\n              <b-col md=\"6\">\n                <b-card-img :src=\"require('../assets/img3.jpg')\" class=\"rounded-0\"></b-card-img>\n              </b-col>\n              <b-col md=\"6\">\n                <b-card-body class=\"text-warning\" title=\"Family\">\n                  <b-card-text class=\"text-dark\">\n                    <h2 class=\"mt-4\">Family Room</h2>\n                    <h6 class=\"mt-4\">4-5 Persons</h6>\n                    <p class=\"mt-4\">Excellent Family Room.</p>\n                  </b-card-text>\n                  <b-button\n                    @click=\"modaldata('Family',800,'warning')\"\n                    size=\"lg\"\n                    class=\"mt-4 text-light\"\n                    block\n                    variant=\"warning\"\n                  >Book</b-button>\n                </b-card-body>\n              </b-col>\n            </b-row>\n          </b-card>\n        </b-col>\n\n        <b-col>\n          <b-card no-body class=\"overflow-hidden\">\n            <b-row no-gutters>\n              <b-col md=\"6\">\n                <b-card-img :src=\"require('../assets/img4.jpg')\" class=\"rounded-0\"></b-card-img>\n              </b-col>\n              <b-col md=\"6\">\n                <b-card-body class=\"text-dark\" title=\"Deluxe\">\n                  <b-card-text class=\"text-dark\">\n                    <h2 class=\"mt-4\">Deluxe Room</h2>\n                    <h6 class=\"mt-4\">1-7 Persons</h6>\n                    <p class=\"mt-4\">Deluxe Room.</p>\n                  </b-card-text>\n                  <b-button\n                    @click=\"modaldata('Deluxe',1000,'dark')\"\n                    size=\"lg\"\n                    class=\"mt-4\"\n                    block\n                    variant=\"dark\"\n                  >Book</b-button>\n                </b-card-body>\n              </b-col>\n            </b-row>\n          </b-card>\n        </b-col>\n      </b-row>\n    </b-container>\n\n    <b-modal hide-footer id=\"my-modal\" :title=\"form.room\">\n      <b-form @submit=\"onSubmit\">\n        <b-form-group id=\"input-group-1\" label=\"Firstname:\" label-for=\"input-1\">\n          <b-form-input\n            id=\"input-1\"\n            v-model=\"form.firstname\"\n            type=\"text\"\n            required\n            placeholder=\"Enter firstname\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group id=\"input-group-1\" label=\"Lastname:\" label-for=\"input-1\">\n          <b-form-input\n            id=\"input-1\"\n            v-model=\"form.lastname\"\n            type=\"text\"\n            required\n            placeholder=\"Enter Lastname\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group id=\"input-group-1\" label=\"Nationality:\" label-for=\"input-1\">\n          <b-form-input\n            id=\"input-1\"\n            v-model=\"form.nationality\"\n            type=\"text\"\n            required\n            placeholder=\"Enter Nationality\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group id=\"input-group-2\" label=\"Birth:\" label-for=\"input2\">\n          <b-form-input\n            id=\"input-2\"\n            v-model=\"form.birth\"\n            type=\"date\"\n            required\n            placeholder=\"Select Day\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group id=\"input-group-1\" label=\"Phone:\" label-for=\"input-1\">\n          <b-form-input\n            id=\"input-1\"\n            v-model=\"form.phone\"\n            type=\"number\"\n            required\n            placeholder=\"Enter a Phone number\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group id=\"input-group-2\" label=\"E-mail\" label-for=\"input-2\">\n          <b-form-input id=\"input-2\" v-model=\"form.email\" required placeholder=\"Enter Email\"></b-form-input>\n        </b-form-group>\n\n        <b-form-group id=\"input-group-2\" label=\"Day to Book:\" label-for=\"input2\">\n          <b-form-input\n            id=\"input-2\"\n            v-model=\"form.date\"\n            type=\"date\"\n            required\n            placeholder=\"Select Day\"\n          ></b-form-input>\n        </b-form-group>\n\n        <b-form-group id=\"input-group-2\" label=\"Price per Night:\" label-for=\"input-2\">\n          <b-form-input id=\"input-2qa\" v-model=\"form.price\" required disabled></b-form-input>\n        </b-form-group>\n        <b-button class=\"text-white\" block type=\"submit\" :variant=\"color\">Book</b-button>\n      </b-form>\n    </b-modal>\n  </div>\n</template>\n\n<script>\nimport { db } from \"@/firebase\";\nimport Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      form: {\n        email: \"\",\n        firstname: \"\",\n        lastname: \"\",\n        nationality: \"\",\n        birth: \"\",\n        phone: \"\",\n        room: \"\",\n        price: \"\",\n        date: \"\"\n      },\n      color: \"\"\n    };\n  },\n  methods: {\n    onSubmit(evt) {\n      evt.preventDefault();\n\n      db.collection(\"clients\")\n        .add({\n          firstname: this.form.firstname,\n          lastname: this.form.lastname,\n          nationality: this.form.nationality,\n          day: this.form.date,\n          phone: this.form.phone,\n          email: this.form.email,\n          room: this.form.room\n        })\n        .then(docRef => {\n          db.collection(this.form.room)\n            .add({\n              clientID: docRef.id,\n              firstname: this.form.firstname,\n              lastname: this.form.lastname,\n              nationality: this.form.nationality,\n              room: this.form.room,\n              price: this.form.price,\n              day: this.form.date,\n              birth: this.form.birth,\n              phone: this.form.phone,\n              email: this.form.email\n            })\n            .then(d => {\n              Swal.fire(\n                \"Good job! Copy your id Reservation\",\n                `Reservation id: ${d.id}`,\n                \"success\"\n              );\n\n              this.form.email = \"\";\n              this.form.nationality = \"\";\n              this.form.firstname = \"\";\n              this.form.lastname = \"\";\n              this.form.birth = \"\";\n              this.form.phone = \"\";\n            });\n        });\n    },\n    modaldata(room, price, color) {\n      this.$bvModal.show(\"my-modal\");\n      this.form.room = room;\n      this.color = color;\n      this.form.price = price;\n    }\n  }\n};\n</script>\n\n<style scoped>\nimg {\n  height: 300px !important;\n}\n</style>\n"],"sourceRoot":""}]}